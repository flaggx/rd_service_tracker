# backend/.env

NODE_ENV=development
PORT=3001
SESSION_SECRET=change_me

# Postgres (Compose service host is "db")
DATABASE_URL="postgresql://app:app@db:5432/rd_service_tracker?schema=public"

# Optional session cookie overrides
# SESSION_COOKIE_NAME=connect.sid
# COOKIE_SECURE=true
# TRUST_PROXY=true
# SESSION_TABLE_NAME=session

# CORS (enable only when serving frontend from another origin in dev)
ENABLE_CORS=true
FRONTEND_ORIGIN=http://localhost:5173